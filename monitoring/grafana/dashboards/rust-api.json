{
  "uid": "rust-api",
  "title": "Rust Binance API",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Requests / sec",
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 },
      "targets": [
        { "expr": "sum(rate(http_requests_total[1m]))", "datasource": { "type": "prometheus", "uid": "prometheus_ds" } }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Error rate (%)",
      "gridPos": { "x": 8, "y": 0, "w": 8, "h": 6 },
      "targets": [
        { "expr": "100 * sum(rate(http_requests_errors_total[5m])) / sum(rate(http_requests_total[5m]))", "datasource": { "type": "prometheus", "uid": "prometheus_ds" } }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } },
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Latency p95 (s)",
      "gridPos": { "x": 16, "y": 0, "w": 8, "h": 6 },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))", "datasource": { "type": "prometheus", "uid": "prometheus_ds" } }
      ]
    },
    {
      "type": "stat",
      "title": "Coins last count",
      "gridPos": { "x": 0, "y": 6, "w": 6, "h": 6 },
      "targets": [
        { "expr": "max(coins_last_count)", "datasource": { "type": "prometheus", "uid": "prometheus_ds" } }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    }
  ]
}
